<block wx:if="{{$taroCompReady}}">
    <view class="note-view">
        <block wx:if="{{notes}}">
            <view class="note-item" wx:for="{{loopArray8}}" wx:for-item="item" wx:for-index="index">
                <view class="note-header">
                    <view class="note-avatar">
                        <at-avatar compid="{{item.$compid__37}}"></at-avatar>
                        <view class="note-title">{{item.$original.title}}</view>
                    </view>
                    <view class="note-updatetime">{{item.$loopState__temp4}}</view>
                </view>
                <view class="note-content">{{item.$loopState__temp6}}</view>
            </view>
        </block>
    </view>
</block>